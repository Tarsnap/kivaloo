PROG=	mkpairs
SRCS=	main.c

# Utility functions
.PATH.c	:	../../libcperciva/util
SRCS	+=	insecure_memzero.c
SRCS	+=	warnp.c
IDIRS	+=	-I ../../libcperciva/util
.PATH.c	:	../lib
SRCS	+=	mkpair.c
IDIRS	+=	-I ../lib

# Fundamental algorithms
.PATH.c	:	../../libcperciva/alg
SRCS	+=	sha256.c
IDIRS	+=	-I ../../libcperciva/alg

# Debugging options
#CFLAGS	+=	-g
#CFLAGS	+=	-DNDEBUG
#CFLAGS	+=	-DDEBUG
#CFLAGS	+=	-pg

.include <bsd.prog.mk>
